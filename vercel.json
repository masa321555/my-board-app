{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install --production=false",
  "regions": ["hnd1"],
  "functions": {
    "app/api/posts/route.ts": {
      "maxDuration": 10
    },
    "app/api/posts/[id]/route.ts": {
      "maxDuration": 10
    },
    "app/api/user/avatar/route.ts": {
      "maxDuration": 30
    },
    "app/api/auth/[...nextauth]/route.ts": {
      "maxDuration": 10
    }
  }
}